                Style="{StaticResource Button Style}">
                <Button.Background>
                    <ImageBrush ImageSource="/Resources/Img/fd_a.png" Stretch="None" />
                </Button.Background>
            </Button>
            <Button
                Grid.Column="3"
                Width="15"
                Height="15"
                Margin="0,0,5,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Click="MaxButton_Click"
                Style="{StaticResource Button Style}">
                <Button.Background>
                    <ImageBrush ImageSource="/Resources/Img/fd_b.png" />
                </Button.Background>
            </Button>
            <Button
