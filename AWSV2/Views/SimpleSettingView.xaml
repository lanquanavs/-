<Controls:MetroWindow
    x:Class="AWSV2.Views.SimpleSettingView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:converter="clr-namespace:AWSV2.Converter"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:local="clr-namespace:AWSV2.Views"
    xmlns:mcb="clr-namespace:CustomControls;assembly=MultiComboBox"
    xmlns:s="https://github.com/canton7/Stylet"
    xmlns:services="clr-namespace:AWSV2.Services"
    xmlns:vm="clr-namespace:AWSV2.ViewModels"
    Title="系统设置"
    Width="800"
    Height="838"
    BorderThickness="0"
    Icon="/Resources/Img/32.ico"
    ShowCloseButton="False"
    ShowMaxRestoreButton="False"
    ShowMinButton="False"
    ShowTitleBar="False"
    WindowState="Maximized">
    <Controls:MetroWindow.IconTemplate>
        <DataTemplate>
            <iconPacks:PackIconFontAwesome
                Width="{TemplateBinding Width}"
                Height="{TemplateBinding Height}"
                Margin="2"
                Padding="4"
                Foreground="{DynamicResource IdealForegroundColorBrush}"
                Kind="CogSolid" />
        </DataTemplate>
    </Controls:MetroWindow.IconTemplate>

    <Window.Resources>

        <converter:WeighingUnitToBoolConverter x:Key="wutb" />
        <converter:WeighingModeToBoolConverter x:Key="wmtb" />
        <converter:WeighingControlToBoolConverter x:Key="wctb" />
        <converter:WeighFormDisplayModeToBoolConverter x:Key="wftb" />
        <converter:PrintingModeToBoolConverter x:Key="pmtb" />
        <converter:LPRToBoolConverter x:Key="lprtb" />
        <converter:OverloadWarningToBoolConverter x:Key="owtb" />
        <converter:DiscountToBoolConverter x:Key="dtb" />
        <converter:InverseBooleanConverter x:Key="ib" />
        <converter:EnableConverter x:Key="enableb" />
        <converter:BoolToVisibilityConverters x:Key="btvcb" />

        <!--<DataTemplate DataType="{x:Type vm:WeighFormViewModel1}">
            <local:WeighFormView />
        </DataTemplate>-->

        <Style TargetType="Hyperlink">
            <Setter Property="TextBlock.TextDecorations" Value="{x:Null}" />
        </Style>

        <Style TargetType="TextBlock">
            <Setter Property="Margin" Value="0,5" />
            <Setter Property="FontSize" Value="14" />
        </Style>
        <Style TargetType="RadioButton">
            <Setter Property="Margin" Value="0,5,5,5" />
            <Setter Property="FontSize" Value="14" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>

        <Style BasedOn="{StaticResource MetroFlatButton}" TargetType="Button">
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Foreground" Value="Black" />
                    <Setter Property="Background" Value="White" />
                </Trigger>
                <Trigger Property="IsPressed" Value="True">
                    <Setter Property="Foreground" Value="Black" />
                    <Setter Property="Background" Value="White" />
                </Trigger>
                <Trigger Property="IsFocused" Value="True">
                    <Setter Property="Foreground" Value="Black" />
                    <Setter Property="Background" Value="White" />
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style TargetType="TabItem">
            <Setter Property="FontSize" Value="14" />
            <Setter Property="MinWidth" Value="110" />
            <Setter Property="Padding" Value="0,5,0,5" />
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Foreground" Value="Black" />
                    <Setter Property="Background" Value="White" />
                </Trigger>
            </Style.Triggers>

        </Style>

        <Style TargetType="ToggleButton" />


        <Style x:Key="ThisPageButtonStyle" TargetType="Button">
            <Setter Property="OverridesDefaultStyle" Value="True" />
            <Setter Property="Cursor" Value="Hand" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="Margin" Value="0,8,8,0" />
            <Setter Property="Height" Value="60" />
            <Setter Property="Width" Value="Auto" />
            <Setter Property="Foreground" Value="#333333" />
            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border
                            Background="#F5F7F9"
                            BorderBrush="#E0E6EC"
                            BorderThickness="1"
                            CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ThisPageYellowButtonStyle" TargetType="Button">
            <Setter Property="OverridesDefaultStyle" Value="True" />
            <Setter Property="Cursor" Value="Hand" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="Margin" Value="0,8,8,0" />
            <Setter Property="Height" Value="60" />
            <Setter Property="Width" Value="Auto" />
            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border
                            Background="#FEF03E"
                            BorderBrush="#FEF03E"
                            BorderThickness="1"
                            CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>

    <Grid Background="#FFEDEDED">
        <Grid.RowDefinitions>
            <RowDefinition Height="56" />
            <RowDefinition Height="92" />
            <RowDefinition Height="68" />
            <RowDefinition Height="*" />
            <RowDefinition Height="10" />
            <RowDefinition Height="16" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="16" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="16" />
        </Grid.ColumnDefinitions>

        <Grid
            Grid.Row="0"
            Grid.ColumnSpan="3"
            Background="#FFEDEDED">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="61" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="40" />
                <ColumnDefinition Width="40" />
                <ColumnDefinition Width="40" />
                <ColumnDefinition Width="30" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="56" />
            </Grid.RowDefinitions>
            <Image
                Grid.Column="0"
